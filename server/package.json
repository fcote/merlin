{
  "name": "merlin-server",
  "version": "0.1.0",
  "description": "Merlin server",
  "author": "Fabien COTE",
  "license": "GPL-3.0",
  "main": "index.ts",
  "scripts": {
    "copy:config": "ts-node scripts/copyConfig.ts",
    "seed:accounts": "ts-node scripts/seedAccounts.ts",
    "build-ts": "tsc --build tsconfig.json",
    "build": "npm run build-ts && npm run copy:config",
    "start": "node -r ./tsconfig.paths.js ./index.js",
    "start:dev": "TS_NODE_FILES=true SKIP_AUTH=true nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node -r tsconfig-paths/register index.ts",
    "start:debug": "TS_NODE_FILES=true SKIP_AUTH=true ts-node --inspect=9000 --debug-brk --ignore false -r tsconfig-paths/register index.ts",
    "start:noauth": "SKIP_AUTH=true ts-node -r tsconfig-paths/register index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "migrate:dev": "HABX_ENV=local DEBUG=knex:tx knex migrate:latest --knexfile knexfile.ts",
    "migrate:dev:rollback": "HABX_ENV=local knex migrate:rollback --knexfile knexfile.ts",
    "migrate:dev:make": "knex migrate:make --migrations-directory ./migrations -x ts",
    "migrate": "knex migrate:latest --migrations-directory migrations --knexfile knexfile.js",
    "migrate:rollback": "knex migrate:rollback --migrations-directory migrations --knexfile knexfile.js"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@sentry/node": "^6.11.0",
    "@sindresorhus/string-hash": "^1.2.0",
    "apollo-server-koa": "^3.4.0",
    "apollo-server-plugin-base": "^3.3.0",
    "apollo-tracing": "^0.15.0",
    "class-validator": "^0.13.1",
    "convict": "^6.0.1",
    "cron-parser": "^4.0.0",
    "dataloader": "^2.0.0",
    "dayjs": "^1.10.6",
    "got": "^11.8.2",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.12.0",
    "jsdom": "^19.0.0",
    "knex": "^1.0.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-router": "^10.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "node-schedule": "^2.0.0",
    "numeral": "^2.0.6",
    "object-hash": "^2.2.0",
    "objection": "^3.0.0",
    "p-map": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.4",
    "subscriptions-transport-ws": "^0.11.0",
    "tough-cookie": "^4.0.0",
    "tsconfig-paths": "^3.10.1",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "ws": "^8.1.0"
  },
  "devDependencies": {
    "@types/convict": "6.1.1",
    "@types/got": "9.6.12",
    "@types/graphql-fields": "1.3.4",
    "@types/jsdom": "16.2.14",
    "@types/koa": "2.13.4",
    "@types/koa-bodyparser": "4.3.5",
    "@types/koa-compose": "3.2.5",
    "@types/koa-router": "7.4.4",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.24",
    "@types/node-fetch": "2.5.12",
    "@types/node-schedule": "1.3.2",
    "@types/numeral": "2.0.2",
    "@types/object-hash": "2.2.1",
    "@types/p-map": "2.0.0",
    "@types/pg": "8.6.4",
    "@types/shelljs": "0.8.11",
    "@types/tough-cookie": "4.0.1",
    "@types/uuid": "8.3.4",
    "@types/ws": "8.2.2",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-import-helpers": "1.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "prettier-eslint": "13.0.0",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.2",
    "winston-transport": "4.4.2"
  }
}
