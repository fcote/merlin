{
  "name": "merlin-server",
  "version": "0.1.0",
  "description": "Merlin server",
  "author": "Fabien COTE",
  "license": "GPL-3.0",
  "main": "index.ts",
  "scripts": {
    "copy:config": "ts-node scripts/copyConfig.ts",
    "seed:accounts": "ts-node scripts/seedAccounts.ts",
    "build-ts": "tsc --build tsconfig.json",
    "build": "npm run build-ts && npm run copy:config",
    "start": "node -r ./tsconfig.paths.js ./index.js",
    "start:dev": "TS_NODE_FILES=true SKIP_AUTH=true nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node -r tsconfig-paths/register index.ts",
    "start:debug": "TS_NODE_FILES=true SKIP_AUTH=true ts-node --inspect=9000 --debug-brk --ignore false -r tsconfig-paths/register index.ts",
    "start:noauth": "SKIP_AUTH=true ts-node -r tsconfig-paths/register index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "migrate:dev": "HABX_ENV=local DEBUG=knex:tx knex migrate:latest --knexfile knexfile.ts",
    "migrate:dev:rollback": "HABX_ENV=local knex migrate:rollback --knexfile knexfile.ts",
    "migrate:dev:make": "knex migrate:make --migrations-directory ./migrations -x ts",
    "migrate": "knex migrate:latest --migrations-directory migrations --knexfile knexfile.js",
    "migrate:rollback": "knex migrate:rollback --migrations-directory migrations --knexfile knexfile.js"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@sentry/node": "^7.9.0",
    "@sindresorhus/string-hash": "^1.2.0",
    "apollo-server-koa": "^3.10.0",
    "apollo-server-plugin-base": "^3.6.2",
    "class-validator": "^0.13.2",
    "convict": "^6.2.3",
    "cron-parser": "^4.6.0",
    "dataloader": "^2.1.0",
    "dayjs": "^1.11.4",
    "got": "^11.8.5",
    "graphql": "^15.8.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.17.0",
    "graphql-subscriptions": "^2.0.0",
    "jsdom": "^21.0.0",
    "knex": "^2.2.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-router": "^12.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "node-schedule": "^2.1.0",
    "numeral": "^2.0.6",
    "object-hash": "^3.0.0",
    "objection": "^3.0.1",
    "p-map": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.5",
    "subscriptions-transport-ws": "^0.11.0",
    "tough-cookie": "^4.0.0",
    "tsconfig-paths": "^4.1.0",
    "type-graphql": "^1.1.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.1",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/convict": "6.1.1",
    "@types/got": "9.6.12",
    "@types/graphql-fields": "1.3.5",
    "@types/jsdom": "21.1.1",
    "@types/koa": "2.13.6",
    "@types/koa-bodyparser": "4.3.10",
    "@types/koa-compose": "3.2.5",
    "@types/koa-router": "7.4.4",
    "@types/lodash": "4.14.194",
    "@types/node": "18.16.4",
    "@types/node-fetch": "2.6.3",
    "@types/node-schedule": "2.1.0",
    "@types/numeral": "2.0.2",
    "@types/object-hash": "3.0.2",
    "@types/p-map": "2.0.0",
    "@types/pg": "8.6.6",
    "@types/shelljs": "0.8.12",
    "@types/tough-cookie": "4.0.2",
    "@types/uuid": "9.0.1",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "4.2.1",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "prettier-eslint": "15.0.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "winston-transport": "4.5.0"
  }
}
